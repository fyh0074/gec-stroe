<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.co.store.mapper.LoginMapper">
  <select id="selectUser" resultType="jp.co.store.emtity.UserBean">
    select * from loginuser where user_id = #{userid} and password = #{password};
  </select>
</mapper>